{
  "expo": {
    "name": "mvpa-go",
    "scheme": "mvpago",
    "slug": "fs-grand",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "MVPA60 needs access to your Camera to record workout.",
        "NSPhotoLibraryUsageDescription": "This app uses the photo library to upload avatar.",
        "NSLocationWhenInUseUsageDescription": "This app uses location to record your workout.",
        "NSMicrophoneUsageDescription": "MVPA60 needs access to your Microphone to record workout."
      },
      "bundleIdentifier": "com.anonymous.mvpago"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.mvpago",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"]
  }
}
